{
  "name": "express-user-api",
  "version": "0.1.0",
  "description": "A production-ready Express.js API for user management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "service"
  ],
  "author": "Agentic Canon Team <team@agenticcanon.dev>",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "prettier": "^3.2.0",
    "supertest": "^7.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "cors": "^2.8.5",
    "winston": "^3.11.0",
    "zod": "4.1.12"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
